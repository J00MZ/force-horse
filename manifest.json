{
  "name": "Force=Horse",
  "version": "0.1",
  "permissions": [
    "http://www.reddit.com/*",
	"https://www.google.com/*",
	"http://edition.cnn.com/*"
  ],
  
  "content_scripts": [
    {
      "matches": [
		"http://www.reddit.com/*",
		"https://www.google.com/*",
		"http://edition.cnn.com/*"
		],
      "js": ["popup.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "manifest_version": 2
}
